<div class="container-md border">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Str</th>
                <th scope="col">Dex</th>
                <th scope="col">Con</th>
                <th scope="col">Int</th>
                <th scope="col">Wis</th>
                <th scope="col">Cha</th>
            </tr>
        </thead>
        <tbody>
            <tr>
              <td><%= character.strength %></td>
              <td><%= character.dexterity %></td>
              <td><%= character.constitution %></td>
              <td><%= character.intelligence %></td>
              <td><%= character.wisdom %></td>
              <td><%= character.charisma %></td>
            </tr>
        </tbody>
    </table>

</div>