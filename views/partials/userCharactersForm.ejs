<form action="/room/<%= room.id %>/add_character" method="POST">
    <div>
        <select id="user-characters" name="my_character" required>
            <% console.log(myCharacters.results) %>
            <option value="" disabled selected hidden>Choose Character</option>
            <% for (let myChar of myCharacters) { %>
             <option value="<%= myChar.id %>"><%= myChar.name %> </option>       
           <% } %>
        </select>
    </div>
    <button class="btn btn-primary"type="submit">Add Character</button>
</form>